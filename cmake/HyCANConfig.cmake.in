@PACKAGE_INIT@

set(CMAKE_CXX_STANDARD @PROJECT_CXX_STANDARD@)
set(CMAKE_CXX_STANDARD_REQUIRED ON)

include(CMakeFindDependencyMacro)

if(NOT TARGET tl::expected)
    find_dependency(tl-expected CONFIG QUIET)
endif()

if(NOT TARGET tl::expected)
    find_path(TL_EXPECTED_INCLUDE_DIR tl/expected.hpp)
    if(NOT TL_EXPECTED_INCLUDE_DIR)
        message(FATAL_ERROR "tl::expected dependency not found. Please install expected-devel or provide tl::expected target.")
    endif()
    add_library(tl::expected INTERFACE IMPORTED)
    set_target_properties(tl::expected PROPERTIES
            INTERFACE_INCLUDE_DIRECTORIES "${TL_EXPECTED_INCLUDE_DIR}"
    )
endif()

if(NOT TARGET HyCAN::HyCAN)
  include("${CMAKE_CURRENT_LIST_DIR}/HyCANTargets.cmake")
endif()